\def\define#1{\textbf{``#1''}}
\let\falsum\bot

\let\propersubset\subset
\let\subset\subseteq
\let\propersupset\supset
\let\supset\supseteq

\DeclareMathOperator\diam{diam}

\DeclareMathOperator\Fin{Fin} % Finite Power Set

\DeclareMathOperator\dom{dom} % domain of a function
\DeclareMathOperator\id{id} % identity function

% Some standard sets
\def\CC{\mathbb{C}}
\def\FF{\mathbb{F}}
\def\NN{\mathbb{N}}
\def\QQ{\mathbb{Q}}
\def\RR{\mathbb{R}}
\def\ZZ{\mathbb{Z}}

% topological stuff
\newcommand\closure[1]{\overline{#1}}
\let\boundary\partial

% vector
\let\og@vector\vec
\let\vector\vec
\def\vec#1{\ensuremath{\mathchoice
                     {\mbox{\boldmath$\displaystyle#1$}}
                     {\mbox{\boldmath$\textstyle#1$}}
                     {\mbox{\boldmath$\scriptstyle#1$}}
                     {\mbox{\boldmath$\scriptscriptstyle#1$}}}}

\newcommand\grad{\boldsymbol{\nabla}}
\def\D{\mathrm{d}}
\newcommand\transpose[1]{#1^{\mathsf{T}}}

\endinput